{
  "name": "@radicalcondor/internal",
  "description": "Internal package for Radical Condor Config repositories",
  "version": "0.3.0",
  "license": "MIT",
  "private": true,
  "main": "src/index.js",
  "keywords": [
    "radicalcondor",
    "prettier",
    "prettier-config"
  ],
  "author": {
    "name": "Radical Condor",
    "url": "https://github.com/radicalcondor"
  },
  "homepage": "https://github.com/radicalcondor/config",
  "repository": {
    "type": "git",
    "url": "https://github.com/radicalcondor/config.git",
    "directory": "packages/internal"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "precommit": "lint-staged",
    "copy:eslint": "cp ../eslint-config/src/index.json src/eslint.json",
    "copy:lint-staged": "cp ../lintstaged-config/src/index.json src/lint-staged.json",
    "copy:prettier": "cp ../prettier-config/src/index.json src/prettier.json",
    "build": "yarn copy:eslint && yarn copy:lint-staged && yarn copy:prettier",
    "lint": "prettier src/** --write"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=4",
    "@typescript-eslint/parser": ">=4",
    "eslint": ">=3",
    "eslint-config-airbnb-base": ">=14",
    "eslint-config-prettier": ">=7",
    "eslint-plugin-import": ">=2",
    "eslint-plugin-jest": ">=24",
    "eslint-plugin-prettier": ">=3",
    "typescript": ">=4"
  },
  "devDependencies": {
    "@radicalcondor/lintstaged-config": "^0.1.4",
    "@radicalcondor/prettier-config": "^0.1.4",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "ajv-cli": "^5",
    "eslint": "^7",
    "eslint-config-airbnb-base": "^14",
    "eslint-config-prettier": "^8",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jest": "^24",
    "eslint-plugin-prettier": "^3",
    "jest": "^26",
    "lint-staged": "^10",
    "prettier": "^2",
    "rimraf": "^3",
    "typescript": "^4"
  }
}
